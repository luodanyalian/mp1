@import '../../../common/base.wxss';
@import '/components/background-audio/background-audio.wxss';
page {
  overflow: hidden;
  width: 100%;
  height: 100%;
}
.not_complete {
   position: relative; 
  width: 100%;
  height: 100%;
}
.tip-text {
  margin: 0 auto;
  position:absolute;
  top:60rpx;
  background-color: rgba(0, 0, 0, .5);
  padding:10rpx 30rpx;
  z-index: 104;
  border-radius: 50rpx;
  font-size: 30rpx;
  color: #fff;
  left: 190rpx;
}
.bg {
  position: absolute;
  top:0px;
  z-index: 0;
  width: 100%;
  height: 100%;
}
.my-map {
  width: 100%;
  height: 100%;
  overflow: hidden;
  text-align: center;
}
.movable-image {
  position:relative;
  transform:translateY(50%);
  margin:0 auto;
  left:0px;
  right:0px;
}
.map-border {
  margin: 0 auto;
  left: 30rpx;
  z-index:103;
  width:92%;
  height:65%;
  position:absolute;
  top:30rpx;
  border: 2rpx solid #344864;
}
.title1 {
  font-size:32rpx;
  bottom:320rpx;
  text-align:center;
  vertical-align:middle;
  color:#485d6b;
  width:90%;
  left:5%;
  text-overflow:ellipsis;
  height:44rpx;
  line-height:44rpx
}
.title {
  font-size: 32rpx;
  bottom:280rpx;
  text-align: center;
  vertical-align: middle;
  color: #485d6b;
  width: 90%;
  left: 5%;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
/*语音播放*/
.box {
  position: absolute;
}
.audio-pos {
  left:5%;
  bottom:155rpx;
  width: 90%;
  height: 120rpx;
}
/*底部*/
.btngoback {
  bottom: 5rpx;
  left: 40rpx;
  width: 125rpx;
  height: 125rpx;
  background: url("http://h5.putao.com/seekingImages/map/btn_big_yellow_back_nor@3x.png") no-repeat center;
  background-size: 100% 100%;
}
.tipback {
  bottom: 60rpx;
  left: 20%;
  width: 275rpx; 
  height: 80rpx;
}
.btnsuccess {
  bottom: 10rpx;
  right: 30rpx;
  width: 265rpx;
  height: 120rpx;
  background: url("http://h5.putao.com/seekingImages/map/btn_big_yellow_finish_dis@3x.png") no-repeat center;
  background-size: 100% 100%;
}